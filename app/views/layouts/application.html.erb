<!DOCTYPE html>
<html>
<head>
  <title>Trackdiff</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% flash.each do |name, msg| %>
  <div class="flash" id="flash_<%= name %>">
    <%= msg %>
  </div>
<% end %>

<%= yield %>

</body>
</html>
